{
  "name": "coursition-experiment",
  "version": "1.0.0",
  "description": "Media parsing service with Effect-TS architecture",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev:server": "bun --inspect-wait --watch src/server.ts",
    "start:server": "bun src/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "check": "biome check --write .",
    "typecheck": "tspc --noEmit",
    "build": "tspc",
    "deps": "bun run src/utils/cli-deps.ts"
  },
  "keywords": [
    "effect-ts",
    "media-parsing",
    "subtitles",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/assemblyai": "catalog:ai",
    "@ai-sdk/deepgram": "catalog:ai",
    "@ai-sdk/elevenlabs": "catalog:ai",
    "@ai-sdk/gladia": "catalog:ai",
    "@ai-sdk/groq": "catalog:ai",
    "@ai-sdk/openai": "catalog:ai",
    "@effect/cli": "catalog:effect",
    "@effect/experimental": "catalog:effect",
    "@effect/language-service": "catalog:effect",
    "@effect/opentelemetry": "catalog:effect",
    "@effect/platform": "catalog:effect",
    "@effect/platform-bun": "catalog:effect",
    "@restatedev/restate-sdk": "catalog:core",
    "@restatedev/restate-sdk-clients": "catalog:core",
    "@typed/id": "catalog:effect",
    "ai": "catalog:ai",
    "effect": "catalog:effect"
  },
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@biomejs/biome": "catalog:lint",
    "@effect/vitest": "catalog:test",
    "@types/bun": "catalog:types",
    "@vitest/ui": "catalog:test",
    "ts-patch": "catalog:types",
    "typescript": "catalog:types",
    "ultracite": "catalog:lint",
    "vitest": "catalog:test",
    "yaml": "catalog:other"
  }
}
